<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/test_tips"
        style="@style/itemTextValueStyle"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:text="@string/fast_charger_tip"
        android:paddingLeft="10px"
        android:textColor="@color/red" >
    </TextView>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1.0"
        android:orientation="vertical" >

        <TableRow
            android:id="@+id/TableRow01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/status"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/tv_status"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10px"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>
        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/plugged"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/tv_plugged"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10px"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>

        <TableRow
            android:id="@+id/TableRow02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/protocol"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/tv_get_protocol"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10px"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>

        <TableRow
            android:id="@+id/TableRow03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/voltage"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/tv_battery_voltage"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10px"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>

        <TableRow
            android:id="@+id/TableRow04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/chargervoltageTV"
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/charger_Voltage"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/tv_charger_Voltage"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10px"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>

        <TableRow
            android:id="@+id/TableRow05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/chargerelectronicnoteTV"
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/battery_electronic"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/tv_battery_electronic"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10px"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>

        <!-- SPRD Bug 773805:Show NTC temperature in charger test. @{ -->
        <TableRow
            android:id="@+id/TableRow06"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone">

            <TextView
                android:id="@+id/batterytemperature"
                style="@style/itemTextKeyStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/battery_temperature"
                android:textColor="#ffffff" >
            </TextView>

            <TextView
                android:id="@+id/tv_batterytemperature"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingLeft="10px"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>
        <!-- @} -->

        <TableRow
            android:id="@+id/TableRow07"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
            <TextView
                android:id="@+id/tv_test_result"
                style="@style/itemTextValueStyle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:text="@string/charging_test"
                android:paddingLeft="10px"
                android:visibility="gone"
                android:textColor="#ffffff" >
            </TextView>
        </TableRow>
    </LinearLayout>

</LinearLayout>